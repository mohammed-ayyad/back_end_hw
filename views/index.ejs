<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<style>body{
    text-align: center;
}
    
</style>
<body>
    <h1>open console then click this button to send data </h1>
    <h2>then you will reseve processed data and printed in console</h2>
    <button onclick="f()">click</button>
</body>

<script>
    async function f(){
        let data =  { "message": "Hello " };
        let u= '/data';
        try{
            let response = await fetch(u, {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json;charset=utf-8'
                },
                body: JSON.stringify(data)
            });
             let result = await response.json();
            console.log(result);
        }
catch(e){
    console.log(e)
}
        
    }

</script>

</html>